<?xml version="1.0"?>
<object>
  <order manager="plone.portaltop" skinname="GenwebTheme">
    <viewlet name="genweb.personalbar" insert-before="*"/>
    <viewlet name="plone.header"/>
    <viewlet name="genweb.globalsections"/>
    <viewlet name="genweb.pathbar"/>
  </order>
  <order manager="plone.portalheader" skinname="GenwebTheme">
    <viewlet name="plone.skip_links"/>
    <viewlet name="genweb.header"/>
  </order>

  <hidden manager= "plone.portalheader" skinname="GenwebTheme">
    <viewlet name="plone.personal_bar"/>
    <viewlet name="plone.logo"/>
    <viewlet name="plone.global_sections"/>
    <viewlet name="plone.searchbox"/>
    <viewlet name="plone.app.i18n.locales.languageselector"/>
  </hidden>
  <hidden manager= "plone.abovecontent" skinname="GenwebTheme">
    <viewlet name="plone.path_bar"/>
  </hidden>

</object>
